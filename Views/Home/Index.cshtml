<script>
    var nowRate = 1;
    function changeCurrency(e)
    {
        var rate = parseFloat($(e).val());
        $(".changeCurrencyObj").each(function () {
            var origin = parseFloat($(this).attr('exactVal'));
            $(this).html(Math.round(origin / nowRate * rate));
            $(this).attr('exactVal', origin / nowRate * rate);
        });
        nowRate = rate;
        $("#askingCurrency").html($(e).find("option:selected").text());
    }


</script>